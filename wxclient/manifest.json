{
    "name": "TimeKeeper",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Browser extension to record time spent on job",
    "homepage_url": "https://localhost/",
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "browser_action": {
      "default_icon": "images/icon19.png",
      "default_popup": "src/popup.html"
    },
    "permissions": [
      "http://localhost:8079/"
    ]
}
